{
  "name": "@takamachi/prettier-config",
  "version": "1.0.1",
  "description": "A sharable Prettier config for macchiitaka",
  "keywords": [
    "Prettier"
  ],
  "homepage": "https://github.com/macchiitaka/prettier-config#readme",
  "bugs": {
    "url": "https://github.com/macchiitaka/prettier-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/macchiitaka/prettier-config.git"
  },
  "license": "MIT",
  "author": "macchiitaka <zxcvbnmtacashi@gmail.com>",
  "main": ".prettierrc.js",
  "scripts": {
    "fmt": "run-p fmt:**",
    "fmt:eslint": "eslint **/*.js --fix --cache",
    "fmt:package-json": "sort-package-json",
    "fmt:pretier:json": "prettier --write --tab-width 2 '**/*.json'",
    "fmt:pretier:md": "prettier --write --tab-width 2 '**/*.md'",
    "lint": "run-p lint:**",
    "lint:eslint": "eslint **/*.js --cache"
  },
  "devDependencies": {
    "@takamachi/eslint-config": "^0.0.1",
    "@types/prettier": "^2.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "sort-package-json": "^1.48.1"
  },
  "peerDependencies": {
    "prettier": "^2.2.1"
  }
}
